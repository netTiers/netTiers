		public override bool Update(TransactionManager transactionManager, <%=className%> entity)
		{
			GenericDatabase database = new GenericDatabase(this._connectionString, _providerFactory);
			DbCommand commandWrapper = DbCommandProvider.GetCommandWrapper(database, "<%=owner + ProcedurePrefix + commandStem + UpdateSuffix %>", _useStoredProcedure, _providerInvariantName);
			
