			{%>
			entity.Original<%=GetPropertyName(cols[i])%> = entity.<%=GetPropertyName(cols[i])%>;
			<%}
			
		}%>
			}
		
			reader.Close();
			
			if (commitTransaction) transactionManager.Commit();
						
			entity.AcceptChanges();
	
			return Convert.ToBoolean(results);
		}
			
