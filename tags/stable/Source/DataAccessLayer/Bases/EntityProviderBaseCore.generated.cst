<script runat="template">
private void CreateManyToManySourceAssignments(TableKeySchema tableKey,System.Collections.Generic.List<string> m2mfilled)
{
	StringBuilder sb = new StringBuilder();
	string fkPropertyName = GetKeysName(tableKey.ForeignKeyMemberColumns);
	
	if (m2mfilled.Contains(fkPropertyName)) return;
	
	m2mfilled.Add(fkPropertyName);
	for(int y=0; y < tableKey.PrimaryKeyMemberColumns.Count; y++)
	{
			
			sb.AppendFormat("{0}child.{1} = child.{2}Source.{3};{4}", 
				Tab(8), 
				GetPropertyName(tableKey.ForeignKeyMemberColumns[y]),
				fkPropertyName, 
				GetPropertyName(tableKey.PrimaryKeyMemberColumns[y]),
				Environment.NewLine);	
	}
	
	Response.Write(string.Format("{0}if(child.{1}Source != null){2}", 
		Tab(6), 
		fkPropertyName, 
		Environment.NewLine));
	Response.Write("{0}{{{1}",Tab(6),  Environment.NewLine);
	Response.Write(sb.ToString());
	Response.Write("{0}}}{1}",Tab(6),  Environment.NewLine);
	Response.WriteLine();
}
</script>
