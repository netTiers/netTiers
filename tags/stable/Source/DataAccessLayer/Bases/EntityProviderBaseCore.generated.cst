	    /// <exception cref="ArgumentException">deepLoadType has invalid value.</exception>
		public override void <%= MethodNames.DeepLoad %>(TransactionManager transactionManager, <%= className %> entity, bool deep, DeepLoadType deepLoadType, System.Type[] childTypes, DeepSession innerList)
		{
			if(entity == null)
				return;
			<%	//Generate a Source property for each foreign key relation		
			System.Collections.ArrayList addedList2 = new System.Collections.ArrayList();
