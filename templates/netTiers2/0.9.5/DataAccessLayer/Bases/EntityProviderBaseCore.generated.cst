		<%-- Going to require knowing the foreign key indexes in order to do deep loading. --%>
		<% if ( IncludeRelations && IncludeGetListByFK ) { %>
		#region DeepLoad
		/// <summary>
		/// Deep Loads the <see cref="IEntity"/> object with criteria based of the child 
		/// property collections only N Levels Deep based on the <see cref="DeepLoadType"/>.
		/// </summary>
		/// <remarks>
		/// Use this method with caution as it is possible to DeepLoad with Recursion and traverse an entire object graph.
		/// </remarks>
		/// <param name="transactionManager"><see cref="TransactionManager"/> object</param>
		/// <param name="entity">The <see cref="<%= className %>"/> object to load.</param>
		/// <param name="deep">Boolean. A flag that indicates whether to recursively save all Property Collection that are descendants of this instance. If True, saves the complete object graph below this object. If False, saves this object only. </param>
		/// <param name="deepLoadType">DeepLoadType Enumeration to Include/Exclude object property collections from Load.</param>
		/// <param name="childTypes"><%= className %> Property Collection Type Array To Include or Exclude from Load</param>
		/// <param name="innerList">A Hashtable of child types for easy access.</param>
	    /// <exception cref="ArgumentNullException">entity or childTypes is null.</exception>
	    /// <exception cref="ArgumentException">deepLoadType has invalid value.</exception>
		internal override void DeepLoad(TransactionManager transactionManager, <%= className %> entity, bool deep, DeepLoadType deepLoadType, System.Type[] childTypes, Hashtable innerList)
		{
			if(entity == null)
				return;
			<%	//Generate a Source property for each foreign key relation		
			System.Collections.ArrayList addedList2 = new System.Collections.ArrayList();
			bool showOnce = false;			
			foreach(TableKeySchema tableKey in SourceTable.ForeignKeys)
			{
				// related table must be in the selection
				if (SourceTables.Contains(tableKey.PrimaryKeyTable)) 
				{
				
				string pkClassName = GetClassName(tableKey.PrimaryKeyTable.Name);
				string pkProperty = GetKeysName(tableKey.PrimaryKeyMemberColumns);
				string fkPropertyName = GetKeysName(tableKey.ForeignKeyMemberColumns);
				string compositePropertyName = fkPropertyName + "Source";
				
				if (addedList2.Contains(fkPropertyName))
					continue;
									
				addedList2.Add(fkPropertyName);
				
				string callParams = GetFunctionThisParametersWithNullable(tableKey.ForeignKeyMemberColumns, "entity");
			%>
			<%
			if (showOnce)
			{
			%>
			#region Composite Source Children
			//Fill Source Composite Properties, however, don't call deep load on them.  
			//So they only get filled a single level deep.
			<%
			}			
			%>

			#region <%=compositePropertyName%>
			if (innerList["<%=pkClassName%>"] == null)
				innerList.Add("<%=pkClassName%>", new ArrayList());
				
			if (CanPropertyDeepLoad("<%=pkClassName.Trim()%>", "<%=compositePropertyName%>", deepLoadType, innerList) 
				&& entity.<%=compositePropertyName%> == null)
			{
				object[] pkItems = new object[<%= tableKey.ForeignKeyMemberColumns.Count %>];
				<% for(int i = 0; i < tableKey.ForeignKeyMemberColumns.Count; i++) { 
					ColumnSchema col = tableKey.ForeignKeyMemberColumns[i];
				%>
				pkItems[<%= i %>] = <%=GetObjectPropertyAccessorWithDefault(col, "entity")%>;
				<% } %>
				<%= pkClassName %> tmpEntity = EntityManager.LocateEntity<<%= pkClassName %>>(EntityLocator.ConstructKeyFromPkItems(typeof(<%=  pkClassName %>), pkItems), DataRepository.Provider.EnableEntityTracking);
				if (tmpEntity != null)
					entity.<%=compositePropertyName%> = tmpEntity;
				else
					entity.<%=compositePropertyName%> = DataRepository.<%= GetProviderName(pkClassName) %>.GetBy<%= pkProperty %>(<%= callParams %>);
			
				//track so it's not fetched again
				((ArrayList)innerList["<%=pkClassName%>"]).Add("<%=compositePropertyName%>");
				
				if (deep && entity.<%=compositePropertyName%> != null)
				{
					DataRepository.<%=GetProviderClassName(pkClassName)%>.DeepLoad(transactionManager, entity.<%=compositePropertyName%>, deep, deepLoadType, childTypes, innerList);
				}
			}
			#endregion <%=compositePropertyName%>
				<%	} //end if in SourceTables
				
					if (showOnce)
					{
					%>
					#endregion 
					<%
						showOnce = false;
					}
				} // end foreach
			  %>
			
			// Load Entity through Provider
			<% 
			int ctr = 0;
				
			foreach(CollectionInfo item in GetChildrenCollections(SourceTable, SourceTables))
			{				
				string listTypeName = item.CollectionTypeName.StartsWith("List") ? item.CollectionTypeName : item.CollectionTypeName.Substring(1);
				if (ctr == 0)
					Response.Write("\t\t\t// Deep load child collections  - Call GetBy" + String.Join(string.Empty,item.PkColNames) + " methods when available" + Environment.NewLine);
				ctr++;
			%>
			<% if (item.CollectionRelationshipType == RelationshipType.ManyToMany) { %>
			
			#region <%=item.CollectionName%>
			if (innerList["List<<%=GetClassName(item.SecondaryTable).Trim()%>>"] == null)
				innerList.Add("List<<%=GetClassName(item.SecondaryTable).Trim()%>>", new ArrayList());

			// RelationshipType.ManyToMany
			if (CanPropertyDeepLoad("List<<%=GetClassName(item.SecondaryTable).Trim()%>>", "<%=item.CollectionName%>", deepLoadType, innerList)) 
			{
				entity.<%=item.CollectionName%> = DataRepository.<%=GetProviderClassName(item.SecondaryTable)%>.GetBy<%=item.GetByKeysName%>(transactionManager, <%=item.CallParams%>);			 
		
				#if NETTIERS_DEBUG
				Debug.WriteLine("- property '<%=item.CollectionName%>' loaded.");
				#endif 
		
				//track so it's not fetched again
				((ArrayList)innerList["List<<%=GetClassName(item.SecondaryTable)%>>"]).Add("<%=item.CollectionName%>");
				
				if (deep && entity.<%=item.CollectionName%>.Count > 0)
				{
					DataRepository.<%=GetProviderClassName(item.SecondaryTable)%>.DeepLoad(transactionManager, entity.<%=item.CollectionName%>, deep, deepLoadType, childTypes, innerList);
				}
			}
			#endregion <%=item.CollectionName%>
			
			<% } else if (item.CollectionRelationshipType == RelationshipType.OneToOne) { %>
			
			#region <%=item.CleanName%>
			if (innerList["<%=listTypeName.Trim()%>"] == null)
				innerList.Add("<%=listTypeName.Trim()%>", new ArrayList());

			// RelationshipType.OneToOne
			if (CanPropertyDeepLoad("<%= listTypeName.Trim() %>", "<%=item.CleanName%>", deepLoadType, innerList))
			{
				entity.<%=item.CleanName%> = DataRepository.<%=GetProviderClassName(item.SecondaryTable)%>.<%=item.GetByKeysName%>(transactionManager, <%=item.CallParams%>);
				Trace.WriteLine("- property '<%=item.CleanName%>' loaded.");
			
				//track so it's not fetched again
				((ArrayList)innerList["<%=listTypeName%>"]).Add("<%=item.CleanName%>");
			
				if (deep && entity.<%=item.CleanName%> != null)
				{
					DataRepository.<%=GetProviderClassName(item.SecondaryTable)%>.DeepLoad(transactionManager, entity.<%=item.CleanName%>, deep, deepLoadType, childTypes, innerList);
				}
			}
			#endregion 
			
			<% } else { %>
			
			#region <%=item.CollectionName%>
			if (innerList["<%=listTypeName.Trim()%>"] == null)
				innerList.Add("<%=listTypeName.Trim()%>", new ArrayList());
				
			//Relationship Type One : Many
			if (CanPropertyDeepLoad("<%= listTypeName.Trim() %>", "<%=item.CollectionName%>", deepLoadType, innerList)) 
			{
				#if NETTIERS_DEBUG
				Debug.WriteLine("- property '<%=item.CollectionName%>' loaded.");
				#endif 

				entity.<%=item.CollectionName%> = DataRepository.<%=GetProviderClassName(item.SecondaryTable)%>.<%=item.GetByKeysName%>(transactionManager, <%=item.CallParams%>);

				//track so it's not fetched again
				((ArrayList)innerList["<%=listTypeName%>"]).Add("<%=item.CollectionName%>");
				
				if (deep && entity.<%=item.CollectionName%>.Count > 0)
				{
					DataRepository.<%=GetProviderClassName(item.SecondaryTable)%>.DeepLoad(transactionManager, entity.<%=item.CollectionName%>, deep, deepLoadType, childTypes, innerList);
				}
			}		
			#endregion 
			<% } %>	    
		    <% }//end foreach CollectionInfo%>
		}
		
		#endregion 
		
		<% 	if ( IncludeSave ) { %>
		#region DeepSave

		/// <summary>
		/// Deep Save the entire object graph of the <%= className %> object with criteria based of the child 
		/// Type property array and DeepSaveType.
		/// </summary>
		/// <param name="transactionManager">The transaction manager.</param>
		/// <param name="entity"><%= className %> instance</param>
		/// <param name="deepSaveType">DeepSaveType Enumeration to Include/Exclude object property collections from Save.</param>
		/// <param name="childTypes"><%= className %> Property Collection Type Array To Include or Exclude from Save</param>
		/// <param name="innerList">A Hashtable of child types for easy access.</param>
		internal override void DeepSave(TransactionManager transactionManager, <%= className %> entity, DeepSaveType deepSaveType, System.Type[] childTypes, Hashtable innerList)
		{	
			if (entity == null)
				return;
				
			// Save Root Entity through Provider
			this.Save(transactionManager, entity);
			
			#region Composite Source Properties
			//Save Source Composite Properties, however, don't call deep save on them.  
			//So they only get saved a single level deep.
			<%	
			System.Collections.ArrayList addedList = new System.Collections.ArrayList();
			
			//Generate a Source property for each foreign key relation		
			foreach(TableKeySchema tableKey in SourceTable.ForeignKeys)
			{	
				// related table must be in the selection
				if (SourceTables.Contains(tableKey.PrimaryKeyTable)) {
				
				string pkClassName = GetClassName(tableKey.PrimaryKeyTable.Name);
				string pkProperty = GetKeysName(tableKey.PrimaryKeyMemberColumns);
				string fkPropertyName = GetKeysName(tableKey.ForeignKeyMemberColumns);
				string compositePropertyName = fkPropertyName + "Source";		
				
				if (addedList.Contains(fkPropertyName))
					continue;
					
				addedList.Add(fkPropertyName);
				
			%>
			
			#region <%=compositePropertyName%>
			if ((deepSaveType == DeepSaveType.IncludeChildren && innerList["<%= pkClassName %>"] != null)
				|| (deepSaveType == DeepSaveType.ExcludeChildren && innerList[" <%= pkClassName %>"] == null))
			{
				if (entity.<%=compositePropertyName%> != null )
				{			
					DataRepository.<%= GetProviderName(pkClassName) %>.Save(transactionManager, entity.<%=compositePropertyName%>);
				}
			}
			#endregion 
			<%	} //end if in SourceTables
			} // end foreach
			%>
			#endregion Composite Source Properties

			<%
			int ctr2 = 0;
			foreach(CollectionInfo item in GetChildrenCollections(SourceTable, SourceTables)) { 
				string listTypeName = item.CollectionTypeName.StartsWith("List") ? item.CollectionTypeName : item.CollectionTypeName.Substring(1);

					if (ctr == 0)
						Response.Write("\t\t\t\t//deep save child collections  - Call DeepSave() Methods on Children" + Environment.NewLine);
					ctr++;
			%>

			<%  
			if (item.CollectionRelationshipType == RelationshipType.OneToOne) 
			{
			%>
			#region <%= listTypeName %>
			if ((deepSaveType == DeepSaveType.IncludeChildren && innerList["<%= listTypeName %>"] != null)
				|| (deepSaveType == DeepSaveType.ExcludeChildren && innerList["<%= listTypeName %>"] == null))
			{
			// update each child parent id with the real parent id (mostly used on insert)
			<%
			for(int i=0; i<item.TableKey.ForeignKeyMemberColumns.Count; i++)
			{
				Response.Write("\n\t\t\t\tentity."+ item.CleanName +"." + GetPropertyName(item.TableKey.ForeignKeyMemberColumns[i].Name) + " = entity." + GetPropertyName(item.TableKey.PrimaryKeyMemberColumns[i].Name) + ";");
			}
			%>
			if (entity.<%=item.CleanName%> != null)
				DataRepository.<%=GetProviderClassName(item.SecondaryTable)%>.DeepSave(transactionManager, entity.<%=item.CleanName%>, deepSaveType, childTypes);
			} 
			#endregion 
			<% 
			} 
			else if (item.CollectionRelationshipType == RelationshipType.OneToMany || item.CollectionRelationshipType == RelationshipType.ManyToOne) 
			{ 
			%>
			#region <%= listTypeName %>
			if ((deepSaveType == DeepSaveType.IncludeChildren && innerList["<%= listTypeName %>"] != null)
				|| (deepSaveType == DeepSaveType.ExcludeChildren && innerList["<%= listTypeName %>"] == null))
			{
			// update each child parent id with the real parent id (mostly used on insert)
			foreach(<%=item.CleanName%> child in entity.<%=item.CollectionName %>)
			{
				<% for(int i=0; i<item.TableKey.ForeignKeyMemberColumns.Count; i++)
				{
					Response.Write("\t\t\t\t\tchild." + GetPropertyName(item.TableKey.ForeignKeyMemberColumns[i].Name) + " = entity." + GetPropertyName(item.TableKey.PrimaryKeyMemberColumns[i].Name) + ";");
				}%>
			}
			
			if (entity.<%=item.CollectionName%>.Count > 0)
				DataRepository.<%=GetProviderClassName(item.SecondaryTable)%>.DeepSave(transactionManager, entity.<%=item.CollectionName%>, deepSaveType, childTypes);
			} 
			#endregion 
			<% }
			else if (item.CollectionRelationshipType == RelationshipType.ManyToMany && IncludeManyToMany) 
			{ %>
			#region List<<%= item.SecondaryTable %>>
			if ((deepSaveType == DeepSaveType.IncludeChildren && innerList["List<<%=item.SecondaryTable%>>"] != null)
				|| (deepSaveType == DeepSaveType.ExcludeChildren && innerList["List<<%=item.SecondaryTable%>>"] == null))
			{
				if (entity.<%=item.CollectionName%>.Count > 0)
					DataRepository.<%=GetProviderClassName(item.SecondaryTable)%>.DeepSave(transactionManager, entity.<%=item.CollectionName %>, deepSaveType, childTypes); 
			}
			#endregion 
		<% } %>
		<% }//end foreach CollectionInfo%>
		}
		#endregion
		
		#region Deep  Helper Methods
		///<summary>
		/// Enforces the rules set in order to 
		/// load this property for a given type.
		///</summary>
		protected bool CanPropertyDeepLoad(String key, String property, DeepLoadType deepLoadType, IDictionary innerList) 
        { 
			if ( deepLoadType == DeepLoadType.ExcludeChildren && innerList[key] == null ) 
			{ 
				MarkDeepLoaded(key, property, innerList); 
				return true; 
			} 
 
			if ( deepLoadType == DeepLoadType.IncludeChildren && innerList[key] != null ) 
			{ 
				if (innerList[key] == null)
					innerList[key] = new ArrayList();
					
				if ( !(innerList[key] as IList).Contains(property) ) 
				{ 
					MarkDeepLoaded(key, property, innerList); 
					return true; 
				} 
			} 

           return false; 
        } 
		
		///<summary>
		/// Helper method used to mark a property 
		/// that is already loaded for a given type.
		///</summary>
		protected void MarkDeepLoaded(String key, String property, IDictionary innerList) 
		{ 
			if (innerList == null)
				return;
			
			if (innerList[key] == null) 
			{ 
				innerList[key] = new ArrayList(); 
			} 
			
			(innerList[key] as IList ).Add(property); 
		}
		#endregion 
		<% 	} // end if ( IncludeSave ) %>
		<% } // end if ( Include Relations ) %>
	
	#region DeepLoadTypes
	///<summary>
	/// Enumeration used to expose the different child entity types 
	/// for child properties in <c><%= className %></c>
	///</summary>
	public enum DeepLoadTypes
	{
	<% 
	if ( IncludeRelations && IncludeGetListByFK ) 
	{ 
	   //Generate enum for each child type foreign key relation		
			System.Collections.ArrayList addedList = new System.Collections.ArrayList();
			bool showOnce = false;			
			foreach(TableKeySchema tableKey in SourceTable.ForeignKeys)
			{
				// related table must be in the selection
				if (SourceTables.Contains(tableKey.PrimaryKeyTable)) 
				{
					string pkClassName = GetClassName(tableKey.PrimaryKeyTable.Name);
					string pkProperty = GetKeysName(tableKey.PrimaryKeyMemberColumns);
					string fkPropertyName = GetKeysName(tableKey.ForeignKeyMemberColumns);
					string compositePropertyName =  fkPropertyName + "Source";
				
				if (addedList.Contains(pkClassName))
					continue;
									
				addedList.Add(pkClassName); 	%>
		
		///<summary>
		/// Composite Property for <c><%= pkClassName %></c> at <%= compositePropertyName %>
		///</summary>
		[DeepLoadType(typeof(<%= pkClassName %>))]
		<%= pkClassName %>,
	<%
			} // end if
		} // end foreach
	%><%
	foreach(CollectionInfo item in GetChildrenCollections(SourceTable, SourceTables))
	{	
		if (item.CollectionRelationshipType != RelationshipType.OneToOne)
		{
			if (addedList.Contains(item.CollectionName))
				continue;
			
	%>
		///<summary>
		/// Collection of <c><%= item.PrimaryTable %></c> as <%= item.CollectionRelationshipType.ToString() %> for <%= item.CollectionName  %>
		///</summary>
		[DeepLoadType(typeof(<%= item.CollectionTypeName %>))]
		<%= item.CollectionName %>,
	<% 
		addedList.Add(item.CollectionName);
		}
		else
		{
			if (addedList.Contains(item.CleanName))
				continue;
		%> 
		///<summary>
		/// Entity <c><%= item.CleanName %><c> as <%= item.CollectionRelationshipType.ToString() %> for <%= item.CleanName  %>
		///</summary>
		[DeepLoadType(typeof(<%= item.CleanName %>))]
		<%= item.CleanName %>,
		<%	
		addedList.Add(item.CleanName);
		}
	  }
	}
	%>  
	}
	#endregion 
	} // end class
	
