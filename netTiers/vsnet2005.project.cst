<%@ CodeTemplate Src="CommonSqlCode.cs" Inherits="MoM.Templates.CommonSqlCode" Language="C#" TargetLanguage="Text" Description="Template description here." Debug="False"%>

<%@ Property Name="SourceDatabase" Type="SchemaExplorer.DatabaseSchema" Optional="True" Category="Context" Description="Database that the stored procedures should be based on." %>
<%@ Property Name="SourceTables" Type="SchemaExplorer.TableSchemaCollection" Category="Context" Description="Tables that the stored procedures should be based on." %>
<%@ Property Name="SourceViews" Type="SchemaExplorer.ViewSchemaCollection" Category="Context" Description="Views that the stored procedures should be based on." %>
<%@ Property Name="EnumTables" Type="SchemaExplorer.TableSchemaCollection" Category="Context" Description="Tables that the stored procedures should be based on." %>


<%@ Property Name="BLLGuid" Type="System.String" Category="Style" Description="BLL Project Guid" %>
<%@ Property Name="DALGuid" Type="System.String" Category="Style" Description="DAL Project Guid" %>
<%@ Property Name="DALSqlGuid" Type="System.String" Category="Style" Description="DAL Project Guid" %>
<%@ Property Name="DALWSGuid" Type="System.String" Category="Style" Description="DAL Project Guid" %>
<%@ Property Name="UTGuid" Type="System.String" Category="Context" Description="Unit tests Project Guid" %>
<%@ Property Name="WSGuid" Type="System.String" Category="Context" Description="WebService Project Guid" %>

<%@ Property Name="OutputDirectory" Type="System.String" Category="General" Description="The root output folder."%>

<%@ Property Name="NameSpace" Type="System.String" Category="Style" Description="Root Namespace" %>
<%@ Property Name="BLLNameSpace" Type="System.String" Category="Data" Description="Entities Namespace." %>
<%@ Property Name="DALNameSpace" Type="System.String" Category="Data" Description="DAL Namespace." %>
<%@ Property Name="DALSqlNameSpace" Type="System.String" Category="Data" Description="DAL sql Namespace." %>
<%@ Property Name="DALWSNameSpace" Type="System.String" Category="Data" Description="DAL ws Namespace." %>
<%@ Property Name="UTNameSpace" Type="System.String" Category="Data" Description="UT Namespace." %>
<%@ Property Name="WSNameSpace" Type="System.String" Category="Data" Description="WebService Namespace." %>

<%@ Property Name="IncludeBll" Type="System.Boolean" Category="Option" Description="" %>

<%@ Property Name="IncludeDALBase" Type="System.Boolean" Category="Option" Description="" %>
<%@ Property Name="IncludeSqlClient" Type="System.Boolean" Category="Option" Description="" %>
<%@ Property Name="IncludeWebservice" Type="System.Boolean" Category="Option" Description="" %>
<%@ Property Name="IncludeWebserviceClient" Type="System.Boolean" Category="Option" Description="" %>
<%@ Property Name="IncludeUnitTest" Type="System.Boolean" Default="true" Category="General" Description="Indicates whether the Unit tests should be generated." %>

<%@ Property Name="SingleProject" Type="System.Boolean" Default="true" Category="General" Description="Indicates whether the project contains only one tier." %>

<%@ Property Name="WebServiceUrl" Type="System.String" Category="WebService" Description="The webservice base URL. The asmx filename will be added programmatically." Default="http://localhost/Services/" %>
<%@ Property Name="rootPathWS" Type="System.String" Category="WebService" Description="The folder where the webservice code is generated." Default="" %>

<%@ Property Name="UsePartialClass" Type="System.Boolean" Default="true" Category="General" Description="Indicates if partial class should be generated." %>

<%@ Assembly Name="SchemaExplorer" %>
<%@ Assembly Name="System.Design" %>
<%@ Import Namespace="SchemaExplorer" %>
<%@ Import Namespace="System.Text" %>
<%@ Import Namespace="System.Text.RegularExpressions" %>
<%
	string fileName = string.Empty;
	
	// the assembly and documentation filename
	if (!SingleProject && IncludeBll)
	{
		fileName =  BLLNameSpace; 
	}
	else if (!SingleProject && IncludeDALBase) 
	{
		fileName = DALNameSpace;
	}
	else if (!SingleProject && IncludeSqlClient) 
	{
		fileName = DALSqlNameSpace; 
	}
	else if (!SingleProject && IncludeUnitTest)
	{
		fileName = UTNameSpace;
	}
	else if (!SingleProject && IncludeWebservice)
	{
		fileName = WSNameSpace;
	}
	else if (!SingleProject && IncludeWebserviceClient
)
	{
		fileName = DALWSNameSpace;
	}	
	else
	{
		fileName = NameSpace;
	}
	
	
	string BLLSubFolder = SingleProject  ? BLLNameSpace : string.Empty;
	string DALSubFolder = SingleProject ? DALNameSpace + @"\" : string.Empty;
	string SqlClientSubFolder = (SingleProject ? DALSqlNameSpace + @"\" : string.Empty) + "";
	string WSClientSubFolder = (SingleProject ? DALWSNameSpace + @"\" : string.Empty) + "";
	string UTSubFolder = SingleProject ? UTNameSpace + @"\" : string.Empty;
%>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ProjectType><%if (!SingleProject && IncludeWebservice) Response.Write("Web"); else Response.Write("Local"); %></ProjectType>
    <ProductVersion>8.0.40607</ProductVersion>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>{<% if (IncludeBll) Response.Write(BLLGuid); else if (!SingleProject && IncludeDALBase) Response.Write(DALGuid); else if (!SingleProject && IncludeSqlClient) Response.Write(DALSqlGuid); else if (!SingleProject && IncludeUnitTest) Response.Write(UTGuid); else if (!SingleProject && IncludeWebservice) Response.Write(WSGuid);else if (!SingleProject && IncludeWebserviceClient) Response.Write(DALWSGuid);  %>}</ProjectGuid>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <ApplicationIcon>
    </ApplicationIcon>
    <AssemblyKeyContainerName>
    </AssemblyKeyContainerName>
    <AssemblyName><%=fileName%></AssemblyName>
    <AssemblyOriginatorKeyFile>
    </AssemblyOriginatorKeyFile>
    <DefaultClientScript>JScript</DefaultClientScript>
    <DefaultHTMLPageLayout>Grid</DefaultHTMLPageLayout>
    <DefaultTargetSchema>IE50</DefaultTargetSchema>
    <DelaySign>false</DelaySign>
    <OutputType>Library</OutputType>
    <RootNamespace><%=fileName%></RootNamespace>
    <RunPostBuildEvent>OnBuildSuccess</RunPostBuildEvent>
    <StartupObject>
    </StartupObject>
    <FileUpgradeFlags>
    </FileUpgradeFlags>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <OutputPath>bin\Debug\</OutputPath>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
    <BaseAddress>285212672</BaseAddress>
    <CheckForOverflowUnderflow>false</CheckForOverflowUnderflow>
    <ConfigurationOverrideFile>
    </ConfigurationOverrideFile>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <DocumentationFile>
    </DocumentationFile>
    <DebugSymbols>true</DebugSymbols>
    <FileAlignment>4096</FileAlignment>
    <NoStdLib>false</NoStdLib>
    <NoWarn>
    </NoWarn>
    <Optimize>false</Optimize>
    <RegisterForComInterop>false</RegisterForComInterop>
    <RemoveIntegerChecks>false</RemoveIntegerChecks>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <OutputPath>bin\Release\</OutputPath>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
    <BaseAddress>285212672</BaseAddress>
    <CheckForOverflowUnderflow>false</CheckForOverflowUnderflow>
    <ConfigurationOverrideFile>
    </ConfigurationOverrideFile>
    <DefineConstants>TRACE</DefineConstants>
    <DocumentationFile><%=fileName%>.xml</DocumentationFile>
    <DebugSymbols>false</DebugSymbols>
    <FileAlignment>4096</FileAlignment>
    <NoStdLib>false</NoStdLib>
    <NoWarn>
    </NoWarn>
    <Optimize>true</Optimize>
    <RegisterForComInterop>false</RegisterForComInterop>
    <RemoveIntegerChecks>false</RemoveIntegerChecks>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System"/>
    <Reference Include="System.Data"/>
    <Reference Include="System.Xml"/>
    <Reference Include="System.configuration" />
    
    
    <% if (IncludeDALBase || IncludeSqlClient || IncludeWebserviceClient) {%>
    <Reference Include="Microsoft.Practices.EnterpriseLibrary.Caching, Version=1.1.0.0, Culture=neutral, processorArchitecture=MSIL">
      <SpecificVersion>True</SpecificVersion>
      <HintPath><%=OutputDirectory%>\References\Microsoft.Practices.EnterpriseLibrary.Caching.dll</HintPath>
    </Reference>
    <Reference Include="Microsoft.Practices.EnterpriseLibrary.Common, Version=1.1.0.0, Culture=neutral, processorArchitecture=MSIL">
      <SpecificVersion>True</SpecificVersion>
      <HintPath><%=OutputDirectory%>\References\Microsoft.Practices.EnterpriseLibrary.Common.dll</HintPath>
    </Reference>
    <Reference Include="Microsoft.Practices.EnterpriseLibrary.Configuration, Version=1.1.0.0, Culture=neutral, processorArchitecture=MSIL">
      <SpecificVersion>True</SpecificVersion>
      <HintPath><%=OutputDirectory%>\References\Microsoft.Practices.EnterpriseLibrary.Configuration.dll</HintPath>
    </Reference>
    <Reference Include="Microsoft.Practices.EnterpriseLibrary.Data, Version=1.1.0.0, Culture=neutral, processorArchitecture=MSIL">
      <SpecificVersion>True</SpecificVersion>
      <HintPath><%=OutputDirectory%>\References\Microsoft.Practices.EnterpriseLibrary.Data.dll</HintPath>
    </Reference>
    
    <Reference Include="NetTiers.Configuration.2005, Version=1.1.0.0, Culture=neutral, processorArchitecture=MSIL">
      <SpecificVersion>True</SpecificVersion>
      <Name>NetTiers.Configuration.2005</Name>
      <HintPath><%=OutputDirectory%>\References\NetTiers.Configuration.2005.dll</HintPath>
    </Reference>
     <%}%>
     
     <% if (!SingleProject && !IncludeBll) {%>	   
	<ProjectReference Include="<%=BLLNameSpace%>">
      <Name><%=BLLNameSpace%></Name>
      <Project>{<%=BLLGuid%>}</Project>
      <Package>{FAE04EC0-301F-11D3-BF4B-00C04F79EFBC}</Package>
    </ProjectReference>
	<%}%>
	
	<% if (!SingleProject && (IncludeSqlClient || IncludeWebserviceClient )) { %>
    <ProjectReference Include="<%=DALNameSpace%>">
        <Name><%=DALNameSpace%></Name>
        <Project>{<%=DALGuid%>}</Project>
        <Package>{FAE04EC0-301F-11D3-BF4B-00C04F79EFBC}</Package>
    </ProjectReference>
    <% } %>
	
	<% if (!SingleProject && IncludeWebservice ) { %>
	<ProjectReference Include="<%=DALNameSpace%>">
        <Name><%=DALNameSpace%></Name>
        <Project>{<%=DALGuid%>}</Project>
        <Package>{FAE04EC0-301F-11D3-BF4B-00C04F79EFBC}"</Package>
    </ProjectReference>
	<ProjectReference Include="<%=DALNameSpace + ".SqlClient" %>">
        <Name><%=DALNameSpace + ".SqlClient" %></Name>
        <Project>{<%=DALSqlGuid%>}</Project>
        <Package>{FAE04EC0-301F-11D3-BF4B-00C04F79EFBC}</Package>
    </ProjectReference>
	 <Reference Include="NetTiers.Configuration.2005, Version=1.1.0.0, Culture=neutral, processorArchitecture=MSIL">
        <SpecificVersion>True</SpecificVersion>
        <HintPath><%=OutputDirectory%>\References\NetTiers.Configuration.2005.dll</HintPath>
    </Reference>
	<% } %>
    
    <% if (IncludeUnitTest /* TODO check du gac */ ) 
     { %>
    <Reference Include="nunit.framework">
	    <Name>nunit.framework</Name>
		<HintPath><%=OutputDirectory%>\References\nunit.framework.dll</HintPath>
	</Reference>
    <% if (!SingleProject) { %>                
     <ProjectReference Include="<%=DALNameSpace%>">
        <Name><%=DALNameSpace%></Name>
        <Project>{<%=DALGuid%>}</Project>
        <Package>{FAE04EC0-301F-11D3-BF4B-00C04F79EFBC}"</Package>
    </ProjectReference>
	<ProjectReference Include="<%=DALNameSpace + ".SqlClient" %>">
        <Name><%=DALNameSpace + ".SqlClient" %></Name>
        <Project>{<%=DALSqlGuid%>}</Project>
        <Package>{FAE04EC0-301F-11D3-BF4B-00C04F79EFBC}</Package>
    </ProjectReference>
	 <Reference Include="NetTiers.Configuration.2005, Version=1.1.0.0, Culture=neutral, processorArchitecture=MSIL">
        <SpecificVersion>True</SpecificVersion>
        <HintPath><%=OutputDirectory%>\References\NetTiers.Configuration.2005.dll</HintPath>
    </Reference>
    <Reference Include="Microsoft.Practices.EnterpriseLibrary.Configuration, Version=1.1.0.0, Culture=neutral, processorArchitecture=MSIL">
      <SpecificVersion>True</SpecificVersion>
      <HintPath><%=OutputDirectory%>\References\Microsoft.Practices.EnterpriseLibrary.Configuration.dll</HintPath>
    </Reference>
    <% } // end if !SingleProject %>
    <% } // end if IncludeUnitTest %>  
    	
	
	
	 <% if (IncludeWebservice || IncludeWebserviceClient) { %>
	<Reference Include="System.Web.Services">
      <Name>System.Web.Services</Name>
    </Reference>
    <Reference Include="System.Web">
      <Name>System.Web</Name>
    </Reference>
    <%}%>
	
	</ItemGroup>
	
	<% if (IncludeUnitTest || IncludeWebservice) {%>
	<ItemGroup>
		<None Include="<%=UTNameSpace%>.dll.config">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>
	<ItemGroup>
		<None Include="nettiersconfigdata.config" >
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>
	<ItemGroup>
		<None Include="cachingConfiguration.config" >
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>
	<ItemGroup>
		<None Include="dataconfiguration.config" >
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>
	<% } %>
		
	<ItemGroup>
<%	
	// Loop on each view
	for (int i = 0; i < SourceViews.Count; i++)
	{
		if (IncludeBll)
		{%>
			<Compile Include="<%=BLLSubFolder%>Views\<%=GetClassName(SourceViews[i].Name)%>.cs">
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=BLLSubFolder%>Views\<%=(UsePartialClass ? GetPartialClassName(SourceViews[i].Name) : GetAbstractClassName(SourceViews[i].Name))%>.cs">
			  <DependentUpon><%=GetClassName(SourceViews[i].Name)%>.cs</DependentUpon>
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=BLLSubFolder%>Views\<%=GetCollectionClassName(SourceViews[i].Name)%>.cs">
			  <SubType>Code</SubType>
			</Compile>	
			<Compile Include="<%=BLLSubFolder%>Views\<%=(UsePartialClass ? GetPartialClassName(GetCollectionClassName(SourceViews[i].Name)) : GetAbstractCollectionClassName(SourceViews[i].Name))%>.cs">
			  <DependentUpon><%=GetCollectionClassName(SourceViews[i].Name)%>.cs</DependentUpon>
			  <SubType>Code</SubType>
			</Compile>					
		<%} // endif includeBll
		
		
		if (IncludeDALBase)
		{%>
			<Compile Include="<%=DALSubFolder%>Bases\Views\<%=GetProviderBaseName(SourceViews[i].Name)%>.cs">
			  <SubType>Code</SubType>
			</Compile>
		<%}
				
		if (IncludeSqlClient)
		{%>
			<Compile Include="<%=SqlClientSubFolder%>Views\Sql<%=GetProviderName(SourceViews[i].Name)%>.cs">
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=SqlClientSubFolder%>Views\Sql<%=GetProviderBaseName(SourceViews[i].Name)%>.cs">
			   <DependentUpon>Sql<%=GetProviderName(SourceViews[i].Name)%>.cs</DependentUpon>
			   <SubType>Code</SubType>
			</Compile>
		<%}
		
		if (IncludeWebserviceClient)
		{%>
			<Compile Include="<%=WSClientSubFolder%>Views\Ws<%=GetProviderName(SourceViews[i].Name)%>.cs">
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=WSClientSubFolder%>Views\Ws<%=GetProviderBaseName(SourceViews[i].Name)%>.cs">
			   <DependentUpon>Ws<%=GetProviderName(SourceViews[i].Name)%>.cs</DependentUpon>
			   <SubType>Code</SubType>
			</Compile>
		<%}
		
		if ( IncludeUnitTest) { %>
		<Compile Include="<%=UTSubFolder%>Views\<%=GetRepositoryTestClassName(SourceViews[i].Name)%>.cs">
			<SubType>Code</SubType>
		</Compile>					
		<%}	// endif IncludeUnitTest
	}
			
	// Loop on each table
	for (int i = 0; i < SourceTables.Count; i++)
	{
		if (IncludeBll)
		{%>
			<Compile Include="<%=BLLSubFolder%><%=GetStructName(SourceTables[i].Name)%>.cs">
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=BLLSubFolder%><%=GetClassName(SourceTables[i].Name)%>.cs">
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=BLLSubFolder%><%=(UsePartialClass ? GetPartialClassName(SourceTables[i].Name) : GetAbstractClassName(SourceTables[i].Name))%>.cs">
			  <DependentUpon><%=GetClassName(SourceTables[i].Name)%>.cs</DependentUpon>
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=BLLSubFolder%><%=GetCollectionClassName(SourceTables[i].Name)%>.cs">
			  <SubType>Code</SubType>
			</Compile>	
			<Compile Include="<%=BLLSubFolder%><%=(UsePartialClass ? GetPartialClassName(GetCollectionClassName(SourceTables[i].Name)) : GetAbstractCollectionClassName(SourceTables[i].Name))%>.cs">
			  <DependentUpon><%=GetCollectionClassName(SourceTables[i].Name)%>.cs</DependentUpon>
			  <SubType>Code</SubType>
			</Compile>
			
			<% if (EnumTables.Contains(SourceTables[i])) { %>
				<Compile Include="<%=BLLSubFolder%><%=GetEnumName(SourceTables[i].Name)%>.cs">
				  <SubType>Code</SubType>
				</Compile>
			<% } %>
		
		<%} // endif includeBll
		
		if (IncludeDALBase)
		{%>
			<Compile Include="<%=DALSubFolder%>Bases\<%=GetProviderBaseName(SourceTables[i].Name)%>.cs">
			  <SubType>Code</SubType>
			</Compile>
		<%}
		
		if (IncludeSqlClient)
		{%>
			<Compile Include="<%=SqlClientSubFolder%>Sql<%=GetProviderName(SourceTables[i].Name)%>.cs">
			  <SubType>Code</SubType>
			</Compile>
			
			<Compile Include="<%=SqlClientSubFolder%>Sql<%=GetProviderBaseName(SourceTables[i].Name)%>.cs">
			  <DependentUpon>Sql<%=GetProviderName(SourceTables[i].Name)%>.cs</DependentUpon>
			  <SubType>Code</SubType>
			</Compile>
					
		<%} // endif includeSqlClient
		
		if (IncludeUnitTest && !IsJunctionTable(SourceTables[i])) { %>
			<Compile Include="<%=UTSubFolder%><%=GetRepositoryTestClassName(SourceTables[i].Name)%>.cs">
			  	<SubType>Code</SubType>
			</Compile>	
		<%}	// endif IncludeUnitTest							
		
		if (IncludeWebserviceClient)
		{%>
			<Compile Include="<%=WSClientSubFolder%>Ws<%=GetProviderName(SourceTables[i].Name)%>.cs">
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=WSClientSubFolder%>Ws<%=GetProviderBaseName(SourceTables[i].Name)%>.cs">
			  <DependentUpon>Ws<%=GetProviderName(SourceTables[i].Name)%>.cs</DependentUpon>
			  <SubType>Code</SubType>
			</Compile>
		<%}	// endif IncludeWebservice
	}// endfor SourceTables.Count
%>		
		<Compile Include="<%=OutputDirectory%>\AssemblyInfo.cs">
		  <SubType>Code</SubType>
		</Compile>
			
		<% /* Include the IEntity interface. */ %>
		<% if (IncludeBll) { %>
			<Compile Include="<%=BLLSubFolder%>IEntity.cs">
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=BLLSubFolder%>Filter.cs">
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=BLLSubFolder%>EntityPropertyComparer.cs">
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=BLLSubFolder%>EntityHelper.cs">
			  <SubType>Code</SubType>
			</Compile>
			<Compile Include="<%=BLLSubFolder%>GenericTypeConverter.cs">
			  <SubType>Code</SubType>
			</Compile>
		<% } %>

		<%/* If SqlCLient is wanted, include the sql Utilities classes */ %>	
		<% if (IncludeDALBase) {%>
			<Compile Include="<%=DALSubFolder%>DataRepository.cs">
			  <SubType>Code</SubType>
			</Compile>
			
			<Compile Include="<%=DALSubFolder%>Bases\DataProviderBase.cs">
			  <SubType>Code</SubType>
			</Compile>
			
			<Compile Include="<%=DALSubFolder%>Configuration.cs">
			  <SubType>Code</SubType>
			</Compile>
			
			<Compile Include="<%=DALSubFolder%>Utility.cs">
			  <SubType>Code</SubType>
			</Compile>
			
			<Compile Include="<%=DALSubFolder%>TransactionManager.cs">
			  <SubType>Code</SubType>
			</Compile>
			
		<% } %>
		<% if (IncludeSqlClient) {%>
			<Compile Include="<%=SqlClientSubFolder%>SqlDataProvider.cs">
			  <SubType>Code</SubType>
			</Compile>
			
			<Compile Include="<%=SqlClientSubFolder%>SqlDataRepository.cs">
			  <SubType>Code</SubType>
			</Compile>
						
			<EmbeddedResource Include="<%=SqlClientSubFolder%>Procedures.xml">
		      <SubType>EmbeddedResource</SubType>
		    </EmbeddedResource>
			
			<Compile Include="<%=SqlClientSubFolder%>StoredProcedureProvider.cs">
			  <SubType>Code</SubType>
			</Compile>			
		<%}%>
		
		<%  if (IncludeWebservice) {%>
			<Compile Include="<%=GetClassName(SourceDatabase.Name) + "Services.asmx"%>">
			  <SubType>Form</SubType>
			</Compile>
		<% } %>
		
		<%/* If WebService CLient is wanted, include the web references files */ %>
        <% if (IncludeWebserviceClient) { %>
            <Compile Include="Web References\WsProxy\Reference.cs">
			  <AutoGen>True</AutoGen>
			  <DesignTime>True</DesignTime>
			  <DependentUpon>results.discomap</DependentUpon>
			</Compile>
        <% } %>
		 <AppDesigner Include="Properties\" />
	</ItemGroup>
	<% if (IncludeWebserviceClient) { %>
		<ItemGroup>
			<Compile Include="<%=WSClientSubFolder%>WsDataProvider.cs">
			  <SubType>Code</SubType>
			</Compile>
			
			<!--<Compile Include="<%=WSClientSubFolder%>WsDataRepository.cs">
			  <SubType>Code</SubType>
			</Compile>-->
		</ItemGroup>
		<ItemGroup>
			<WebReferences Include="Web References\" />
		</ItemGroup>
		<ItemGroup>
			<WebReferenceUrl Include="<%=WebServiceUrl%><%=GetClassName(SourceDatabase.Name)%>Services.asmx">
				<UrlBehavior>Static</UrlBehavior>
				<RelPath>Web References\WsProxy\</RelPath>
				<UpdateFromURL><%=WebServiceUrl%><%=GetClassName(SourceDatabase.Name)%>Services.asmx</UpdateFromURL>
				<ServiceLocationURL>
				</ServiceLocationURL>
				<CachedDynamicPropName>
				</CachedDynamicPropName>
				<CachedAppSettingsObjectName>Settings</CachedAppSettingsObjectName>
				<CachedSettingsPropName><%=GetClassName(SourceDatabase.Name)%>Application_WsProxy_<%=GetClassName(SourceDatabase.Name)%>Services</CachedSettingsPropName>
			</WebReferenceUrl>
		</ItemGroup>
		<ItemGroup>
			<None Include="Web References\WsProxy\<%=GetClassName(SourceDatabase.Name)%>Services.disco" />
			<None Include="Web References\WsProxy\<%=GetClassName(SourceDatabase.Name)%>Services.wsdl" />
			<None Include="Web References\WsProxy\results.discomap">
				<Generator>MSDiscoCodeGenerator</Generator>
				<LastGenOutput>Reference.cs</LastGenOutput>
			</None>
		</ItemGroup>
	<% } %>
  <Import Project="$(MSBuildBinPath)\Microsoft.CSHARP.Targets" />
  <PropertyGroup>
    <PreBuildEvent>
    </PreBuildEvent>
    <PostBuildEvent>
    	<% if (IncludeUnitTest) { Response.Write("copy \"$(ProjectDir)*.config\" \"$(TargetDir)\""); } %>
    </PostBuildEvent>
  </PropertyGroup>
</Project>
