            }
            else
            {
                //TODO: You need to add your custom provider render when using a third party RDBMS.
                // Need to load Default XSLT Template for the stored procedures....
            }
 
            TextWriter writer = null;
            XPathDocument mydata = null;

            try
            {
                if(_netTiersSqlProvider)
                {
                    writer = new StreamWriter(rootPathSQL + "\\views.sql");
                    //Create a new XPathDocument and load the XML data to be transformed.
                    mydata = new XPathDocument(rootPathDALSql + "\\Views\\" + xmlOutputName);
                }
                else if(ProviderInvariantName == "System.Data.SqlServerCe.3.5")
