                    mydata = new XPathDocument(rootPathDALGeneric + "\\Views\\" + xmlOutputName);
                }

                xslt.Transform(mydata, null, writer, null);
                writer.Close();
            }
            catch(Exception ex)
            {
                Response.WriteLine(ex);
            }
            finally
            {
                if (writer != null)
